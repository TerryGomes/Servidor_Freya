	<weapon id="3439" name="Shining Canine">
		<set name="name-ru" value="Сверкающие Клыки"/>
		<!--Elit by Nika 26.02.2012-->
		<set name="crystal_type" value="NONE"/>
		<set name="icon" value="icon.etc_pet_arms_i00"/>
		<set name="price" value="34000"/>
		<set name="rnd_dam" value="10"/>
		<set name="soulshots" value="0"/>
		<set name="spiritshots" value="0"/>
		<set name="type" value="PET"/>
		<equip>
			<slot id="WOLF"/>
		</equip>
		<for>
			<add stat="pAtk" order="0x10" value="7"/>
			<add stat="mAtk" order="0x10" value="8"/>
			<set stat="baseCrit" order="0x08" value="80"/>
